<%= form_for :user, url: '/users' do |f| %>
Name: <%= f.text_field :name %><br>
Height: <%= f.number_field :height %><br>
happiness: <%= f.number_field :happiness %><br>
Nauseaus: <%= f.number_field :nausea %><br>
Dollars in pocket <%= f.number_field :tickets %><br>
Check box owner <%= f.check_box :admin %><br>
Password <%= f.password_field :password %><br>
<%= f.submit "Create User" %>
<% end %>
